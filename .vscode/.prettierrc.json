{
  "overrides": [
    {
      "files": ["*.json", "*.jsonc"],
      "options": {
        "parser": "jsonc"
      }
    },
    {
      "files": ["*.yml", "*.yaml"],
      "options": {
        "parser": "yaml"
      }
    }
  ],
  "parser": "typescript",
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none"
}
